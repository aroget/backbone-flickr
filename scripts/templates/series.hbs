<h2>{{collection.[0].photoset.title}}</h2>

<ul class="photo-list">
  
  {{#each collection.[0].photoset.photo}}
    <li class="photo-item">
      <img src="https://farm{{farm}}.staticflickr.com/{{server}}/{{id}}_{{secret}}.jpg">
      
      {{#if title}}
        <caption>Title: <strong>{{title}}</strong></caption>
      {{/if}}

    </li>
  {{/each}}

</ul>

